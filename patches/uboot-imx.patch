diff --git a/drivers/serial/serial_mxc.c b/drivers/serial/serial_mxc.c
index 4f15592e20..1b30ffe1d5 100644
--- a/drivers/serial/serial_mxc.c
+++ b/drivers/serial/serial_mxc.c
@@ -179,6 +179,7 @@ static void _mxc_serial_init(struct mxc_uart *base, int use_dte)
 	writel(0, &base->ts);
 }
 
+#ifndef CONFIG_WINDOWS_BSP
 static void _mxc_serial_setbrg(struct mxc_uart *base, unsigned long clk,
 			       unsigned long baudrate, bool use_dte)
 {
@@ -208,6 +209,66 @@ static void _mxc_serial_setbrg(struct mxc_uart *base, unsigned long clk,
 
 	writel(UCR1_UARTEN, &base->cr1);
 }
+#else
+#define MAX(a,b) (((a)>(b))?(a):(b))
+
+static unsigned long mxc_serial_get_rfdiv_reg_val(unsigned long rfdiv)
+{
+	switch (rfdiv) {
+	case 1: return 5;
+	case 2: return 4;
+	case 3: return 3;
+	case 4: return 2;
+	case 5: return 1;
+	case 6: return 0;
+	case 7: return 6;
+	default:
+		return 0;
+	}
+}
+
+static void _mxc_serial_setbrg(struct mxc_uart *base, unsigned long clk,
+					unsigned long baudrate, bool use_dte)
+{
+	u32 tmp;
+	u32 rfdiv;
+
+	_mxc_serial_flush(base);
+
+	rfdiv = clk / (16 * baudrate);
+	if (rfdiv > 7) {
+		rfdiv = 7;
+	}
+
+	tmp = mxc_serial_get_rfdiv_reg_val(rfdiv) << UFCR_RFDIV_SHF;
+	if (use_dte)
+		tmp |= UFCR_DCEDTE;
+	tmp |= (TXTL << UFCR_TXTL_SHF) | (RXTL << UFCR_RXTL_SHF);
+	writel(tmp, &base->fcr);
+
+	unsigned long num = clk;
+	unsigned long denom = 16 * rfdiv * baudrate;
+	unsigned long scale = (MAX(num, denom) + 65535) / 65536;
+	unsigned long ubmrPlusOne = num / scale;
+	unsigned long ubirPlusOne = denom / scale;
+
+	writel(ubirPlusOne - 1, &base->bir);
+	writel(ubmrPlusOne - 1, &base->bmr);
+
+	writel(UCR2_WS | UCR2_IRTS | UCR2_RXEN | UCR2_TXEN | UCR2_SRST,
+			&base->cr2);
+
+	/*
+	 * setting the baudrate triggers a reset, returning cr3 to its
+	 * reset value but UCR3_RXDMUXSEL "should always be set."
+	 * according to the imx8 reference-manual
+	 */
+	writel(readl(&base->cr3) | UCR3_RXDMUXSEL, &base->cr3);
+
+	writel(UCR1_UARTEN, &base->cr1);
+}
+
+#endif
 
 #if !CONFIG_IS_ENABLED(DM_SERIAL)
 
